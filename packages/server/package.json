{
  "name": "@browserbasehq/stagehand-server",
  "version": "3.6.0",
  "description": "Stagehand API server",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "pnpm --filter @browserbasehq/stagehand-server run --parallel \"/^(build:esm-tests|build:server:dist|gen:openapi|build:sea:esm)$/\"",
    "dev": "tsx watch src/server.ts",
    "build:esm-tests": "pnpm -w --dir ../.. exec tsc -p packages/server/tsconfig.tests.json",
    "build:server:dist": "pnpm -w --dir ../.. exec tsc -p packages/server/tsconfig.json && pnpm -w --dir ../.. exec tsc-alias -p packages/server/tsconfig.json",
    "build:sea:esm": "tsx scripts/build-sea.ts --mode=esm",
    "build:sea:cjs": "tsx scripts/build-sea.ts --mode=cjs",
    "lint": "cd ../.. && prettier --check packages/server && cd packages/server && eslint . && pnpm run typecheck",
    "typecheck": "pnpm -w --dir ../.. exec tsc -p packages/server/tsconfig.json --noEmit",
    "test": "pnpm -w --dir ../.. exec turbo run test:server --filter=@browserbasehq/stagehand-server --",
    "test:server": "tsx scripts/test-server.ts",
    "test:integration": "pnpm run test:server -- packages/server/dist/tests/integration",
    "test:integration:local": "STAGEHAND_SERVER_TARGET=local pnpm run test:server -- packages/server/dist/tests/integration",
    "test:integration:sea": "STAGEHAND_SERVER_TARGET=sea pnpm run test:server -- packages/server/dist/tests/integration",
    "gen:openapi": "tsx scripts/gen-openapi.ts"
  },
  "dependencies": {
    "@browserbasehq/sdk": "^2.4.0",
    "@browserbasehq/stagehand": "workspace:*",
    "@fastify/cors": "^11.0.1",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@t3-oss/env-core": "^0.13.8",
    "fastify": "^5.3.2",
    "fastify-metrics": "^12.1.0",
    "fastify-plugin": "^4.5.1",
    "fastify-zod-openapi": "^5.5.0",
    "http-status-codes": "^2.3.0",
    "pino": "^9.7.0",
    "pino-pretty": "^11.3.0",
    "playwright": "1.52.0",
    "uuid": "^11.0.5",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/node": "22.13.1",
    "eslint": "^9.7.0",
    "eslint-plugin-security": "^3.0.1",
    "openai": "4.87.1",
    "postject": "1.0.0-alpha.6",
    "prettier": "^3.2.5",
    "source-map": "^0.7.4",
    "tsc-alias": "^1.8.10",
    "tsx": "*",
    "vitest": "^4.0.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/browserbase/stagehand.git",
    "directory": "packages/server"
  },
  "bugs": {
    "url": "https://github.com/browserbase/stagehand/issues"
  },
  "homepage": "https://stagehand.dev"
}
